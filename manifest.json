{
    "manifest_version": 3,
    "name": "Panabit Deep Linker (Pro)",
    "version": "2.1",
    "permissions": [
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_title": "Configure Panabit Monitor",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/panabit16.png",
            "24": "images/panabit24.png",
            "32": "images/panabit32.png"
        }
    },
    "icons": {
        "16": "images/panabit16.png",
        "32": "images/panabit32.png",
        "48": "images/panabit64.png",
        "128": "images/panabit128.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}