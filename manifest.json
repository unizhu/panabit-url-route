{
    "manifest_version": 3,
    "name": "Panabit Deep Linker (Pro)",
    "version": "2.0",
    "permissions": [
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_icon": {
            "256": "./images/panabit256.png",
            "128": "./images/panabit128.png",
            "64": "./images/panabit64.png",
            "32": "./images/panabit32.png",
            "24": "./images/panabit24.png",
            "16": "./images/panabit16.png"
        },
        "default_popup": "popup.html",
        "default_title": "Configure Panabit Monitor"
    },
    "icons": {
        "256": "./images/panabit256.png",
        "128": "./images/panabit128.png",
        "64": "./images/panabit64.png",
        "32": "./images/panabit32.png",
        "24": "./images/panabit24.png",
        "16": "./images/panabit16.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}